  "licenses": [
    {
      "type": "proprietary",
      "url": "https://raw.github.com/newrelic/node-newrelic/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "jsdoc": "^3.4.0",
    "newrelic": "^1.28.1",
    "tap": "^5.7.3"
  },
  "dependencies": {
    "lodash": "^4.13.1"
  }
}
